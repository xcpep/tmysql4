ifndef config
  config=release32
endif
export config

PROJECTS := gm_cmd

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

gm_cmd: 
	@echo "==== Building gm_cmd ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f gm_cmd.make

clean:
	@${MAKE} --no-print-directory -C . -f gm_cmd.make clean

help:
